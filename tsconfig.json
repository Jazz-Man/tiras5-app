    "sourceMap": true,
    "jsx": "react",
    "importHelpers": true,
    "downlevelIteration": true,
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "removeComments": true,
    "resolveJsonModule":true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
